    <!-- http://example.org/dbc#decodedFrom -->

    <owl:ObjectProperty rdf:about="http://example.org/dbc#decodedFrom">
        <rdfs:domain rdf:resource="http://example.org/dbc#SignalLog"/>
        <rdfs:range rdf:resource="http://example.org/dbc#MessageLog"/>
        <rdfs:comment>Indicates that a SignalLog is derived by decoding a specific MessageLog observation.</rdfs:comment>
        <rdfs:label>decoded from</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.org/dbc#decodedVia -->

    <owl:ObjectProperty rdf:about="http://example.org/dbc#decodedVia">
        <rdfs:domain rdf:resource="http://example.org/dbc#Signal"/>
        <rdfs:range rdf:resource="http://example.org/dbc#SignalEncoding"/>
        <rdfs:comment>Links a CAN signal definition to the SignalEncoding that defines how its bit-level representation is decoded into a physical or logical value.</rdfs:comment>
        <rdfs:label>decoded via</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.org/dbc#encodedVia -->

    <owl:ObjectProperty rdf:about="http://example.org/dbc#encodedVia">
        <rdfs:domain rdf:resource="http://example.org/dbc#Message"/>
        <rdfs:range rdf:resource="http://example.org/dbc#SignalEncoding"/>
        <rdfs:comment>Links a CAN message definition to the SignalEncoding that specifies how its payload bytes are structured and interpreted.</rdfs:comment>
        <rdfs:label>encoded via</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.org/dbc#hasReceiver -->

    <owl:ObjectProperty rdf:about="http://example.org/dbc#hasReceiver">
        <rdfs:domain rdf:resource="http://example.org/dbc#Signal"/>
        <rdfs:range rdf:resource="http://example.org/dbc#Node"/>
        <rdfs:comment>Links a CAN signal definition to the node (ECU) that consumes or interprets this signal.</rdfs:comment>
        <rdfs:label>has receiver</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.org/dbc#hasSignal -->

    <owl:ObjectProperty rdf:about="http://example.org/dbc#hasSignal">
        <rdfs:domain rdf:resource="http://example.org/dbc#Message"/>
        <rdfs:range rdf:resource="http://example.org/dbc#Signal"/>
        <rdfs:comment>Indicates that a CAN message contains a particular signal within its payload structure.</rdfs:comment>
        <rdfs:label>has signal</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.org/dbc#hasTransmitter -->

    <owl:ObjectProperty rdf:about="http://example.org/dbc#hasTransmitter">
        <rdfs:domain rdf:resource="http://example.org/dbc#Message"/>
        <rdfs:range rdf:resource="http://example.org/dbc#Node"/>
        <rdfs:comment>Links a CAN message definition to the node (ECU) responsible for transmitting it on the bus.</rdfs:comment>
        <rdfs:label>has transmitter</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.org/dbc#isPartOf -->

    <owl:ObjectProperty rdf:about="http://example.org/dbc#isPartOf">
        <rdfs:domain rdf:resource="http://example.org/dbc#Signal"/>
        <rdfs:range rdf:resource="http://example.org/dbc#Message"/>
        <rdfs:comment>Indicates that a signal is a structural component of a particular CAN message definition.</rdfs:comment>
        <rdfs:label>is part of</rdfs:label>
    </owl:ObjectProperty>
    


